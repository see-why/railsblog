<div class="all-post-div">
  <%= link_to(user_post_path(@user, post)) do |post, index|%>
    <div class="post-div">
      <p><strong>Post #<%= index + 1 %></strong></p>
      <h3><%= post.title %></h3>
      <p> <%= post.text %> </p>
    </div>
  <% end %>  
 <%= render post.five_recent_comments %>
</div>